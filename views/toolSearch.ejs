  <section class="row">
    <div class="col-sm-1">
    </div>
    <div class="col-sm-10 basicToolDiv text-center">
      <h2 id="toolH2">12 basic woodworking tools</h2><br>
       <% if (basicTools) { %>
        <% basicTools.forEach(function(value, index) { %>
          <div class="well col-sm-4 custDiv">
            <h2 class="toolsearchH2"><%= value.title %></h2>
            <a href="http://www.startwoodworking.com/lists/12-basic-hand-tools-woodworking"><img src="<%= value.image %>"></a>
          </div>
        <% }); %>
        <a href="http://www.wagnermeters.com/wood-moisture-meter/top-40-woodworking-tools/" class="link40">Another 40 good tools to have</a>
      <% } %>
    </div>
    <div class="col-sm-1">
    </div>
  </section>
